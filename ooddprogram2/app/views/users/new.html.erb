<p id="notice"><%= notice %></p>
<h1>New User</h1>

<%= render 'form', user: @user %>

<% if logged_in? %>
  <%if logged_in_admin? %>
    <%= link_to 'Back to List of All users', users_path %>
  <% end %>
  <%= link_to "Log out", logout_path, method: :delete %>
<% else %>
  <%= link_to 'Home', root_path %>

<% end %>

